<Window x:Class="DNSCsharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DNSCsharp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="ButtName" Content="Get Host Name" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="376" Height="400" FontFamily="Showcard Gothic" FontSize="36" Background="#FF6DA31B" Click="ButtClickHost"/>
        <Button x:Name="ButtIp" Content="Get IP Address" HorizontalAlignment="Left" Margin="381,10,0,0" VerticalAlignment="Top" Width="403" Height="400" FontFamily="Showcard Gothic" FontSize="36" Background="#FF0A2265" Click="ButtClickIp"/>

    </Grid>
</Window>
